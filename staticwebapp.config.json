{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/css/*", "/js/*", "/images/*", "/*.svg", "/*.png", "/*.ico", "/*.json", "/*.wasm", "/*.woff", "/*.woff2"]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".wasm": "application/wasm"
  }
}
